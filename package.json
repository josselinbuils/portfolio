  "scripts": {
    "analyse": "ANALYSE=true vite build",
    "build": "rimraf build && yarn build:client && yarn build:server && node prerender.mjs && rimraf dist && cpx \"build/**/*\" dist && rimraf build",
    "build:client": "vite build",
    "build:server": "vite build --ssr src/entry-server.tsx",
    "dev": "ts-node-dev -r tsconfig-paths/register --project ./server/tsconfig.json ./server/devServer.ts",
    "lint": "yarn lint:code && yarn lint:typings",
    "lint:code": "eslint .",
    "lint:typings": "tsc",
    "pre-commit": "exec-staged",
    "prepare": "husky install",
    "start": "ts-node -r tsconfig-paths/register --project ./server/tsconfig.json ./server/server.ts"
  },
