{
  "name": "portfolio-react",
  "version": "0.1.0",
  "homepage": "https://josselinbuils.me/portfolio-react",
  "private": true,
  "scripts": {
    "analyze": "yarn build-client && source-map-explorer 'dist/client/static/js/*.js'",
    "build": "yarn build-server && yarn build-client",
    "build-client": "react-app-rewired build && rimraf dist/client && cpx \"build/**/*\" dist/client && rimraf build",
    "build-server": "tsc -p ./server/tsconfig.json && rimraf dist/server && cpx \"tmp/**/*\" dist/server && rimraf tmp",
    "build-server-dev": "tsc -p server/tsconfig.dev.json",
    "start": "node dist/server/server.js",
    "dev-client": "react-app-rewired start",
    "dev-server": "yarn build-server-dev && concurrently --kill-others 'yarn build-server-dev -w' 'nodemon dist/server/server.js'"
  },
  "dependencies": {
    "@josselinbuils/utils": "^0.1.2",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/request-promise-native": "^1.0.16",
    "@types/snoowrap": "^1.15.3",
    "@types/source-map-support": "^0.5.0",
    "ansi_up": "^4.0.3",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cpx": "^1.5.0",
    "dayjs": "^1.8.13",
    "express": "^4.16.4",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "serve-static": "^1.13.2",
    "snoowrap": "^1.15.2",
    "source-map-support": "^0.5.12",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.0",
    "source-map-explorer": "^1.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/app/$1"
    }
  }
}
