{
  "name": "portfolio",
  "homepage": "https://josselinbuils.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "add:staged": "exec-staged 'git add' '\\.(js|md|tsx?|scss)$'",
    "analyze": "ANALYZE=true yarn build",
    "csscomb:staged": "exec-staged 'csscomb -tv' '\\.scss$'",
    "dev": "next",
    "build": "next build",
    "lint": "eslint .",
    "lint:staged": "exec-staged 'yarn lint' '\\.tsx?$'",
    "prettier:staged": "exec-staged 'prettier --write' '\\.(js|md|tsx?|scss)$'",
    "start": "next start",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged && yarn prettier:staged && yarn csscomb:staged && yarn add:staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@josselinbuils/utils": "^0.2.3",
    "ansi_up": "^4.0.4",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.6",
    "dicom-parser": "^1.8.7",
    "diff-match-patch": "^1.0.5",
    "js-untar": "^2.0.0",
    "jsonschema": "^1.4.0",
    "lodash.clonedeep": "^4.5.0",
    "next": "^10.0.2",
    "next-transpile-modules": "^4.1.0",
    "prettier": "^2.2.0",
    "prismjs": "^1.22.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "remark-gfm": "^1.0.0",
    "sass": "^1.29.0",
    "snoowrap": "^1.21.0",
    "tar-stream": "^2.1.4",
    "typescript": "^4.1.2",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@josselinbuils/eslint-config-react": "^0.1.0",
    "@josselinbuils/exec-staged": "^0.1.1",
    "@next/bundle-analyzer": "^10.0.2",
    "@types/classnames": "^2.2.11",
    "@types/diff-match-patch": "^1.0.32",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/prettier": "^2.1.5",
    "@types/prismjs": "^1.16.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/tar-stream": "^2.1.0",
    "@types/webpack-env": "^1.15.3",
    "@types/ws": "^7.4.0",
    "csscomb": "^4.3.0",
    "eslint": "^7.13.0",
    "husky": "^4.3.0"
  },
  "engines": {
    "node": ">=14"
  }
}
