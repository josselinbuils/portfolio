{
  "name": "portfolio-react",
  "version": "0.1.0",
  "homepage": "https://josselinbuils.me",
  "private": true,
  "scripts": {
    "add:staged": "exec-staged 'git add' '\\.(js|tsx?|scss)$'",
    "analyze": "ANALYZE=true yarn build:client",
    "build": "yarn build:server && yarn build:client",
    "build:client": "react-app-rewired build && react-snap && rimraf dist/client && cpx \"build/**/*\" dist/client && rimraf build",
    "build:server": "tsc -p ./server/tsconfig.json && rimraf dist/server && rimraf dist/src && cpx \"tmp/**/*\" dist && rimraf tmp",
    "build:server:dev": "tsc -p server/tsconfig.dev.json",
    "csscomb:staged": "exec-staged 'csscomb -tv' '\\.scss$'",
    "dev:client": "react-app-rewired start",
    "dev:server": "concurrently --kill-others 'yarn build:server:dev -w' 'nodemon --watch dist/server dist/server/server.js'",
    "lint": "yarn lint:client && yarn lint:server",
    "lint:client": "tslint -p tsconfig.json",
    "lint:server": "tslint -p server/tsconfig.json",
    "lint:staged": "exec-staged 'yarn lint:client' '^src.*\\.tsx?$' 'yarn lint:server' '^server.*\\.ts$'",
    "prettier:staged": "exec-staged 'prettier --write' '\\.(js|tsx?|scss)$'",
    "start": "node dist/server/server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:staged && yarn prettier:staged && yarn csscomb:staged && yarn add:staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@josselinbuils/utils": "^0.1.2",
    "@types/classnames": "^2.2.10",
    "@types/diff-match-patch": "^1.0.32",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^13.13.5",
    "@types/prettier": "^2.0.0",
    "@types/prismjs": "^1.16.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/request-promise-native": "^1.0.17",
    "@types/snoowrap": "^1.19.0",
    "@types/source-map-support": "^0.5.1",
    "@types/ws": "^7.2.4",
    "ansi_up": "^4.0.4",
    "body-parser": "^1.19.0",
    "circular-dependency-plugin": "^5.2.0",
    "classnames": "^2.2.6",
    "cpx": "^1.5.0",
    "dayjs": "^1.8.26",
    "dicom-parser": "^1.8.5",
    "diff-match-patch": "^1.0.4",
    "express": "^4.17.1",
    "js-untar": "^2.0.0",
    "jsonschema": "^1.2.6",
    "lodash.clonedeep": "^4.5.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-snap": "^1.23.0",
    "rimraf": "^3.0.2",
    "serve-static": "^1.14.1",
    "snoowrap": "^1.21.0",
    "source-map-support": "^0.5.19",
    "typescript": "~3.8.3",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@josselinbuils/exec-staged": "^0.1.1",
    "@josselinbuils/tslint": "^5.1.0",
    "concurrently": "^5.2.0",
    "csscomb": "^4.3.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.2",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "last 3 Edge versions",
    "last 1 Safari versions"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "singleQuote": true
  },
  "reactSnap": {
    "inlineCss": true,
    "puppeteerArgs": [
      "--no-sandbox"
    ]
  }
}
