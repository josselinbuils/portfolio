{
  "name": "portfolio",
  "homepage": "https://josselinbuils.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "add:staged": "exec-staged 'git add' '\\.(js|tsx?|scss)$'",
    "analyze": "ANALYZE=true yarn build",
    "csscomb:staged": "exec-staged 'csscomb -tv' '\\.scss$'",
    "dev": "next",
    "build": "next build",
    "lint": "tslint -p tsconfig.json",
    "lint:staged": "exec-staged 'yarn lint' '\\.tsx?$'",
    "prettier:staged": "exec-staged 'prettier --write' '\\.(js|tsx?|scss)$'",
    "start": "next start",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type-check && yarn lint:staged && yarn prettier:staged && yarn csscomb:staged && yarn add:staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@josselinbuils/utils": "^0.1.2",
    "@types/classnames": "^2.2.10",
    "@types/diff-match-patch": "^1.0.32",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/prettier": "^2.0.2",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/tar-stream": "^2.1.0",
    "@types/ws": "^7.2.6",
    "add": "^2.0.6",
    "ansi_up": "^4.0.4",
    "circular-dependency-plugin": "^5.2.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.29",
    "dicom-parser": "^1.8.5",
    "diff-match-patch": "^1.0.5",
    "js-untar": "^2.0.0",
    "jsonschema": "^1.2.6",
    "lodash.clonedeep": "^4.5.0",
    "next": "^9.4.4",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "snoowrap": "^1.21.0",
    "tar-stream": "^2.1.3",
    "typescript": "^3.9.6",
    "ws": "^7.3.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@josselinbuils/exec-staged": "^0.1.1",
    "@josselinbuils/tslint": "^5.1.0",
    "@next/bundle-analyzer": "^9.4.4",
    "csscomb": "^4.3.0",
    "husky": "^4.2.5",
    "tslint": "^6.1.2"
  },
  "engines": {
    "node": ">=14"
  }
}
